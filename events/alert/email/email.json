{
  "name": "Email Security Alert",
  "type": "email_alert",
  "extends": "alert",
  "description": "Email Security alert events report the detections and resolutions of email malicious activity.",
  "attributes": {
    "$include": "includes/email.json",
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Isolated"
        },
        "4": {
          "name": "Quarantined"
        },
        "5": {
          "name": "Tagged"
        }
      }
    },
    "quarantine_uid": {
      "group": "primary",
      "requirement": "optional"
    }
  }
}